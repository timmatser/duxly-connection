# Shopify API Key (public, used for App Bridge)
VITE_SHOPIFY_API_KEY=your_api_key_here

# App identifier for multi-app support
# This identifies which Shopify app registration to use (e.g., duxly-connection, duxly-connection-hart-beach)
VITE_APP_ID=duxly-connection

# Backend API URL
VITE_API_URL=https://your-api-gateway-url.amazonaws.com/prod
