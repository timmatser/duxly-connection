# Shopify App Configuration
SHOPIFY_API_KEY=your_api_key_here
SHOPIFY_API_SECRET=your_api_secret_here

# App URL (API Gateway URL after deployment)
APP_URL=https://your-api-id.execute-api.eu-central-1.amazonaws.com/prod

# AWS Configuration
AWS_REGION=eu-central-1
AWS_ACCOUNT_ID=your_account_id

# Frontend URL (CloudFront URL after deployment)
FRONTEND_URL=https://your-distribution-id.cloudfront.net

# Parameter Store Configuration
PARAMETER_STORE_PREFIX=/shopify/clients
